/* How to check a triangle or not triangle */
 
#include <bits/stdc++.h>
using namespace std;
 
int main(){
int a, b, c;
    cout<<"please input the values: ";
    cin>>a>>b>>c;
    if((a+b)>c && (b+c)>a && (a+c)>b){
         cout<<"This is a triangle\n";
        }else{
            cout<<"This is not a triangle\n";
        }

        return 0;
}
